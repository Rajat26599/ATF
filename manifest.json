{
    "name": "ATF",
    "description": "All Time Favourite",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "options_page": "options.html",
    "action": {
        "default_popup": "popup.html"
    },
    "incognito": "split"
}